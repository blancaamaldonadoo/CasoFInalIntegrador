package Animal;

public class Animal_Acuatico{
    boolean aletas;
    boolean branquias;
    boolean escamas;
    String tipo_acuatico;

    public Animal_Acuatico(boolean aletas, boolean cola, boolean escamas, String tipo_acuatico, String nombre, String especie, int id, boolean salud, boolean comportamiento, boolean alimentacion){
        super(nombre, especie, id, salud, comportamiento, alimentacion);
        this.aletas = aletas;
        this.cola = cola;
        this.escamas = escamas;
        this.tipo_acuatico = tipo_acuatico;
    }
}