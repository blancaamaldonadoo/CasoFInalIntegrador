package Animales;

public class Animal_Terrestre{
    boolean patas;
    boolean cola;
    boolean pelo;
    String tipo_terrestre;

public Animal_Terrestre(boolean patas, boolean cola, boolean pelo, String tipo_terrestre, String nombre, String especie, int id, boolean salud, boolean comportamiento, boolean alimentacion){
    super(nombre, especie, id, salud, comportamiento, alimentacion);
    this.patas = patas;
    this.cola = cola;
    this.pelo = pelo;
    this.tipo_terrestre = tipo_terrestre;
}
}